<script src="//code.jquery.com/jquery-3.3.1.min.js"></script>

<!-- bundles -->
{{{assets.master.scripts}}}

<script defer src="//unpkg.com/popper.js/dist/umd/popper.min.js"></script>
<script defer src="//unpkg.com/bootstrap-material-design@4.1.1/dist/js/bootstrap-material-design.min.js"></script>
<link as="style" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons" onload="this.onload=null;this.rel='stylesheet'" rel="preload">
<script defer src="//use.fontawesome.com/releases/v5.0.6/js/all.js"></script>

<!-- component assets -->
{{#each component}}
	{{#if assets.styles}}
		{{#each assets.styles}}
			<link as="style" href="{{this}}" onload="this.onload=null;this.rel='stylesheet'" rel="preload">
		{{/each}}
	{{/if}}
{{/each}}
{{#each component}}
	{{#if assets.scripts}}
		{{#each assets.scripts}}
			<script async defer src="{{this}}"></script>
		{{/each}}
	{{/if}}
{{/each}}